from collections import defaultdict

class Graph:
    def __init__(self):
        self.graph = defaultdict(list)

    def add_edge(self, u, v):
        self.graph[u].append(v)

    def dfs(self, start):
        visited = set()
        self._dfs(start, visited)

    def _dfs(self, u, visited):
        visited.add(u)
        print(u, end=' ')

        for v in self.graph[u]:
            if v not in visited:
                self._dfs(v, visited)

    def bfs(self, start):
      visited = set()
      queue = []

      visited.add(start)
      queue.append(start)

      while queue:
        u = queue.pop(0)
        print(u, end=' ')

        for v in self.graph[u]:
          if v not in visited:
            queue.append(v)
            visited.add(v)

                    
                    
# User input for graph
